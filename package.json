{
  "name": "starter",
  "version": "1.1.0",
  "description": "Laravel Mix, Tailwind, Vue / Vanilla starter",
  "repository": "github:croxton/laravel-mix-tailwind-starter",
  "author": "Mark Croxton",
  "license": "ISC",
  "scripts": {
    "dev": "npm run clean && mix watch --hot",
    "check": "npm run clean && SOURCE_MAPS=true LINT=true && mix watch --hot",
    "build": "npm run clean && mix --production",
    "clean": "rimraf web/dist/* web/{,**/}*{json}",
    "fix-scripts": "eslint --fix 'src/scripts/**/*.{vue,js,jsx,mjs}'"
  },
  "dependencies": {
    "@iconfu/svg-inject": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.11",
    "axios": "^0.21.1",
    "core-js": "^3.3.6",
    "focus-visible": "^5.2.0",
    "jquery": "^3.6.0",
    "laravel-mix-alias": "^1.0.2",
    "lazy-scripts": "^0.3.2",
    "lazysizes": "^5.2.2",
    "node-sass-utils": "^1.1.2",
    "postcss-loader": "^5.3.0",
    "pubsub-js": "^1.7.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.0.3",
    "chokidar": "^3.5.1",
    "copy-webpack-plugin": "^5.0.4",
    "eslint": "^7.26.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-vue": "^7.5.0",
    "faker": "^4.1.0",
    "globby": "^10.0.1",
    "imagemin-webpack-plugin": "^2.4.2",
    "laravel-mix": "^6.0.19",
    "laravel-mix-eslint": "^0.2.0",
    "laravel-mix-imagemin": "1.0.3",
    "laravel-mix-make-file-hash": "^2.2.0",
    "laravel-mix-polyfill": "^3.0.0",
    "laravel-mix-purgecss": "^6.0.0",
    "laravel-mix-svg-sprite": "^1.0.1",
    "node-sass": "^4.14.1",
    "postcss": "^8.2.15",
    "postcss-preset-env": "^6.7.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.32.13",
    "sass-loader": "^11.1.1",
    "tailwindcss": "^2.1.2",
    "vue-loader": "^16.2.0",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.37.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR",
    "not ie 10",
    "not ie 10",
    "not op_mini all",
    "not op_mob 12.1",
    "not baidu 7.12",
    "not ie_mob 10",
    "not ie_mob 11",
    "not and_uc 11.8"
  ],
  "eslintConfig": {
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module"
    },
    "plugins": [
      "compat"
    ],
    "rules": {
      "compat/compat": "warn"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "useTabs": false,
    "tabWidth": 4,
    "semi": false,
    "singleQuote": false
  }
}
