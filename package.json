    "laravel-mix-make-file-hash": "^2.2.0",
    "laravel-mix-polyfill": "^3.0.1",
    "laravel-mix-svg-sprite": "^1.0.2",
    "npm": "^8.19.2",
    "postcss": "^8.4.18",
    "postcss-preset-env": "^7.8.2",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "stylelint": "^14.14.0",
    "stylelint-config-standard": "^29.0.0",
    "svgo": "^1.3.2",
    "tailwindcss": "^3.2.1",
