# GULP and SASS
# --------------------------------------------------------------------------- #

node_modules/*
.sass-cache/*


# Composer
# --------------------------------------------------------------------------- #

/vendor


# Build artefacts            
# --------------------------------------------------------------------------- #

/web/dist/*
/node_modules/*
/yarn-error.log
/npm-debug.log
/mix-manifest.json
/web/mix-manifest.json
/web/hot


# Caches
# --------------------------------------------------------------------------- #

sized/
thumbs/
_thumbs/
/bak/*
/web/cache/*

# CraftCMS environment
# --------------------------------------------------------------------------- #

.env.php
.env.sh
.env


# CraftCMS Caches
# --------------------------------------------------------------------------- #

/storage/backups/*
!/storage/backups/.gitignore

/storage/logs/*
!/storage/logs/.gitignore

/storage/runtime/*
!/storage/runtime/.gitignore

/web/cpresources/*


# CraftCMS User Uploads
# --------------------------------------------------------------------------- #

/web/uploads/*
!/web/uploads/index.html


# CraftCMS plugins
# --------------------------------------------------------------------------- #

/web/imager/*


# Dev
# --------------------------------------------------------------------------- #

/_dev/*


